    "browserslist": "^4.20.3",
    "jest": "^27.4.6",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.72.0",
